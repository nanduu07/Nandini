-- Part 0: Drop Tables If They Already Exist (to avoid duplication errors) 
DROP TABLE IF EXISTS Courses; 
DROP TABLE IF EXISTS Departments; -- Part A: Create Tables with Normalization (up to 3NF) 
CREATE TABLE Departments ( 
dept_id INT PRIMARY KEY, 
dept_name VARCHAR(50) UNIQUE 
); 
CREATE TABLE Courses ( 
course_id INT PRIMARY KEY, 
course_name VARCHAR(100), 
dept_id INT, 
FOREIGN KEY (dept_id) REFERENCES Departments(dept_id) 
); -- Part B: Insert Sample Data into Department and Course Tables 
INSERT INTO Departments (dept_id, dept_name) VALUES 
